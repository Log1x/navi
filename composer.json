{
  "name": "log1x/navi",
  "type": "package",
  "license": "MIT",
  "description": "A simple alternative to WordPress' NavWalker",
  "authors": [
    {
      "name": "Brandon Nifong",
      "email": "brandon@tendency.me"
    }
  ],
  "keywords": [
    "wordpress",
    "navwalker"
  ],
  "support": {
    "issues": "https://github.com/log1x/navi/issues"
  },
  "autoload": {
    "psr-4": {
      "Log1x\\Navi\\": "src/"
    }
  },
  "require": {
    "php": ">=7.1.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4"
  },
  "extra": {
    "acorn": {
      "providers": [
        "Log1x\\Navi\\NaviServiceProvider"
      ],
      "aliases": {
        "Navi": "Log1x\\Navi\\Facades\\Navi"
      }
    }
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor --extensions=php --standard=PSR12 ."
    ]
  }
}
